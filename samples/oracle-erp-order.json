        "LineTotal": 500.00,
        "RequestedDate": "2025-01-20",
        "Warehouse": "WH-EAST"
      },
      {
        "LineNumber": 2,
        "ItemNumber": "PROD-002",
        "ItemDescription": "Gadget Type B",
        "Quantity": 25,
        "UOM": "EA",
        "UnitPrice": 30.00,
        "LineTotal": 750.00,
        "RequestedDate": "2025-01-20",
        "Warehouse": "WH-EAST"
      }
    ],
    "ShipTo": {
      "Name": "ACME Warehouse",
      "Address1": "123 Main Street",
      "Address2": "Building 5",
      "City": "New York",

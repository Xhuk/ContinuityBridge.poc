      "Country": "USA",
      "Phone": "+1-555-0123"
    }
  },
  "metadata": {
    "version": "R12",
    "endpoint": "/fscmRestApi/resources/11.13.18.05/orders",
    "documentation": "https://docs.oracle.com/en/cloud/saas/supply-chain-management/",
    "fields": {
      "OrderNumber": {
        "description": "Unique order identifier",
        "required": true,
        "format": "SO-XXXXX"
      },
      "Priority": {
        "description": "Order priority level",
        "required": true,
        "enum": ["1", "2", "3", "4"]
      },
      "Status": {
        "description": "Order status",
        "required": true,
        "enum": ["BOOKED", "SHIPPED", "CANCELLED", "PENDING"]
      },
      "UOM": {
        "description": "Unit of measure",
        "required": true,
        "enum": ["EA", "BOX", "CASE", "PALLET"]
      }
    }
  }
}

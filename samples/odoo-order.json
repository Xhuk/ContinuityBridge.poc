    "amount_total": 1250.00,
    "currency_id": [1, "USD"],
    "order_line": [
      {
        "id": 100,
        "product_id": [20, "SKU001"],
        "name": "Product A",
        "product_uom_qty": 10,
        "product_uom": [1, "Unit"],
        "price_unit": 50.00,
        "price_subtotal": 500.00
      },
      {
        "id": 101,
        "product_id": [21, "SKU002"],
        "name": "Product B",
        "product_uom_qty": 25,
        "product_uom": [1, "Unit"],
        "price_unit": 30.00,
        "price_subtotal": 750.00
      }
    ],
    "partner_shipping_id": [11, "ACME Warehouse"],
    "partner_street": "123 Main Street",
    "partner_street2": "Building 5",
    "partner_city": "New York",

    "partner_zip": "10001",
    "partner_country_id": [233, "United States"],
    "partner_phone": "+1-555-0123"
  },
  "metadata": {
    "version": "17.0",
    "endpoint": "/xmlrpc/2/object",
    "documentation": "https://www.odoo.com/documentation/",
    "fields": {
      "name": {
        "description": "Order reference/number",
        "required": true,
        "format": "auto-generated or custom"
      },
      "priority": {
        "description": "Order priority",
        "required": false,
        "enum": ["0", "1"],
        "defaultValue": "0"
      },
      "state": {
        "description": "Order workflow state",
        "required": true,
        "enum": ["draft", "sent", "sale", "done", "cancel"]
      },
      "partner_id": {
        "description": "Customer ID [id, name]",
        "required": true,
        "format": "[integer, string]"
      }
    }
  }
}
